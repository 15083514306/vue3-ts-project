#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules/commitizen/bin/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules/commitizen/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules/commitizen/bin/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules/commitizen/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/commitizen@4.3.0/node_modules:/Users/caichanghuan/my-work/vue3-ts-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitizen/bin/commitizen" "$@"
else
  exec node  "$basedir/../commitizen/bin/commitizen" "$@"
fi
